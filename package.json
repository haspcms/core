{
  "name": "@haspcms/core",
  "version": "0.0.4",
  "files": [
    "dist/**",
    "README.md"
  ],
  "main": "./dist/core.umd.cjs",
  "module": "./dist/core.js",
  "exports": {
    ".": {
      "import": "./dist/core.js",
      "require": "./dist/core.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf dist",
    "build": "vite build",
    "clean": "rm -rf node_modules && rm -rf dist",
    "preview": "vite preview",
    "prepare": "husky && husky install"
  },
  "lint-staged": {
    "**/*.(ts|tsx|js)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "eslint-plugin-next": "^0.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.4.1",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "prettier": "^3.4.2",
    "prettier-plugin-organize-imports": "^4.1.0",
    "vite": "^6.0.5"
  },
  "peerDependencies": {
    "axios": "^1.2.2",
    "jsona": "^1.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.0.0"
  }
}
